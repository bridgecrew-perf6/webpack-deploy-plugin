{
  "name": "@planjs/webpack-deploy-plugin",
  "version": "0.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "stan-builder --bundler babel --cjs --runtime",
    "dev": "npm run prepare -- --watch",
    "prepublishOnly": "npm run release",
    "release": "./scripts/release.sh",
    "lint:fix": "eslint --fix --ext js,ts src",
    "test": "jest"
  },
  "keywords": [
    "webpack-plugin",
    "planjs",
    "oss-upload-tool",
    "rsync",
    "deploy",
    "webpack-deploy-plugin"
  ],
  "files": [
    "lib"
  ],
  "author": "fupeng <fupenglxy@gmail.com> (https://github.com/fupengl)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@planjs/fabric": "^0.0.90",
    "@types/node": "^17.0.21",
    "@types/webpack": "^5.28.0",
    "babel-jest": "^27.5.1",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "memfs": "^3.4.1",
    "stan-builder": "^0.14.3",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "standard-version": "^9.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planjs/webpack-deploy-plugin.git"
  },
  "homepage": "https://github.com/planjs/webpack-deploy-plugin#readme",
  "engines": {
    "node": ">=6.9"
  },
  "peerDependencies": {
    "webpack": ">=4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "multimatch": "5.0.0",
    "node-ssh": "^12.0.3",
    "oss-upload-tool": "^0.1.0",
    "rsync": "^0.6.1",
    "schema-utils": "^4.0.0"
  }
}
